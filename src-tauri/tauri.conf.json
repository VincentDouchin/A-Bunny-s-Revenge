{
	"$schema": "../node_modules/@tauri-apps/cli/schema.json",
	"build": {
		"beforeBuildCommand": "npm run build",
		"beforeDevCommand": "npm run dev",
		"devPath": "http://localhost:5173",
		"distDir": "../dist"
	},
	"package": {
		"productName": "fabled_recipes",
		"version": "0.1.0"
	},
	"tauri": {
		"allowlist": {
			"all": false,
			"window": {
				"all": true
			}
		},
		"bundle": {
			"active": true,
			"category": "DeveloperTool",
			"copyright": "",
			"deb": {
				"depends": []
			},
			"externalBin": [],
			"icon": [
				"../public/pwa-512x512.png",
				"../public/pwa-192x192.png",
				"../public/pwa-64x64.png",
				"../public/favicon.ico"
			],
			"identifier": "fabled-recipes",
			"longDescription": "",
			"macOS": {
				"entitlements": null,
				"exceptionDomain": "",
				"frameworks": [],
				"providerShortName": null,
				"signingIdentity": null
			},
			"resources": [],
			"shortDescription": "",
			"targets": "all",
			"windows": {
				"certificateThumbprint": null,
				"digestAlgorithm": "sha256",
				"timestampUrl": ""
			}
		},
		"security": {
			"csp": null
		},
		"updater": {
			"active": false
		},
		"windows": [
			{
				"fullscreen": true,
				"height": 600,
				"resizable": true,
				"title": "Fabled recipes",
				"width": 800
			}
		]
	}
}
