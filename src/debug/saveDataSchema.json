{
	"$ref": "#/definitions/SaveData",
	"$schema": "http://json-schema.org/draft-07/schema#",
	"definitions": {
		"Item": {
			"additionalProperties": false,
			"properties": {
				"name": {
					"$ref": "#/definitions/items"
				},
				"quantity": {
					"type": "number"
				}
			},
			"required": [
				"name",
				"quantity"
			],
			"type": "object"
		},
		"SaveData": {
			"additionalProperties": false,
			"properties": {
				"acorns": {
					"type": "number"
				},
				"crops": {
					"additionalProperties": {
						"additionalProperties": false,
						"properties": {
							"name": {
								"$ref": "#/definitions/crops"
							},
							"stage": {
								"type": "number"
							}
						},
						"required": [
							"stage",
							"name"
						],
						"type": "object"
					},
					"type": "object"
				},
				"inventories": {
					"additionalProperties": {
						"items": {
							"$ref": "#/definitions/Item"
						},
						"type": "array"
					},
					"type": "object"
				},
				"modifiers": {
					"items": {
						"type": "string"
					},
					"type": "array"
				},
				"playerPosition": {
					"items": {
						"type": "number"
					},
					"type": "array"
				},
				"playerRotation": {
					"items": {
						"type": "number"
					},
					"type": "array"
				},
				"quests": {
					"additionalProperties": false,
					"properties": {
						"grandma_1": {
							"items": {
								"type": "boolean"
							},
							"type": "array"
						},
						"jack_1": {
							"items": {
								"type": "boolean"
							},
							"type": "array"
						}
					},
					"type": "object"
				},
				"selectedSeed": {
					"anyOf": [
						{
							"type": "null"
						},
						{
							"$ref": "#/definitions/crops"
						}
					]
				},
				"settings": {
					"additionalProperties": false,
					"properties": {
						"fullscreen": {
							"type": [
								"boolean",
								"null"
							]
						},
						"mute": {
							"type": "boolean"
						},
						"volume": {
							"type": "number"
						}
					},
					"required": [
						"volume",
						"mute",
						"fullscreen"
					],
					"type": "object"
				},
				"unlockedPaths": {
					"type": "number"
				}
			},
			"required": [
				"crops",
				"playerPosition",
				"playerRotation",
				"quests",
				"selectedSeed",
				"inventories",
				"modifiers",
				"settings",
				"unlockedPaths",
				"acorns"
			],
			"type": "object"
		},
		"crops": {
			"enum": [
				"carrot",
				"beet",
				"tomato",
				"lettuce",
				"pumpkin",
				"wheat"
			],
			"type": "string"
		},
		"items": {
			"enum": [
				"acorn",
				"beet",
				"beetroot_salad",
				"beet_seeds",
				"butter",
				"carrot",
				"carrot_cake",
				"carrot_seeds",
				"carrot_soup",
				"cinnamon",
				"cookie",
				"egg",
				"flan",
				"flour",
				"ham",
				"ham_honey",
				"honey",
				"honey_glazed_carrot",
				"lettuce",
				"lettuce_seeds",
				"magic_bean",
				"milk",
				"parsley",
				"pumpkin",
				"pumpkin_bowl",
				"pumpkin_bread",
				"pumpkin_seeds",
				"roasted_carrot",
				"slime_bread",
				"slime_dough",
				"slime_dumpling",
				"steak",
				"strawberry",
				"strawberry_pie",
				"sugar",
				"tomato",
				"tomato_seeds",
				"tomato_soup",
				"wheat",
				"wheat_seeds"
			],
			"type": "string"
		}
	}
}