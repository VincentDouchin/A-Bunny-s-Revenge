{
	"$ref": "#/definitions/SaveData",
	"$schema": "http://json-schema.org/draft-07/schema#",
	"definitions": {
		"Item": {
			"additionalProperties": false,
			"properties": {
				"name": {
					"$ref": "#/definitions/items"
				},
				"quantity": {
					"type": "number"
				},
				"recipe": {
					"$ref": "#/definitions/items"
				}
			},
			"required": [
				"name",
				"quantity"
			],
			"type": "object"
		},
		"SaveData": {
			"additionalProperties": false,
			"properties": {
				"acorns": {
					"type": "number"
				},
				"crops": {
					"additionalProperties": {
						"additionalProperties": false,
						"properties": {
							"luck": {
								"type": "number"
							},
							"name": {
								"$ref": "#/definitions/crops"
							},
							"planted": {
								"type": "number"
							},
							"stage": {
								"type": "number"
							},
							"watered": {
								"type": "boolean"
							}
						},
						"required": [
							"luck",
							"name",
							"planted",
							"stage",
							"watered"
						],
						"type": "object"
					},
					"type": "object"
				},
				"daytime": {
					"additionalProperties": false,
					"properties": {
						"current": {
							"type": "number"
						},
						"dayLight": {
							"type": "number"
						},
						"dayToNight": {
							"type": "boolean"
						},
						"timePassed": {
							"type": "number"
						}
					},
					"required": [
						"current",
						"dayToNight",
						"timePassed",
						"dayLight"
					],
					"type": "object"
				},
				"inventories": {
					"additionalProperties": {
						"items": {
							"$ref": "#/definitions/Item"
						},
						"type": "array"
					},
					"type": "object"
				},
				"modifiers": {
					"items": {
						"$ref": "#/definitions/items"
					},
					"type": "array"
				},
				"playerPosition": {
					"items": {
						"type": "number"
					},
					"type": "array"
				},
				"playerRotation": {
					"items": {
						"type": "number"
					},
					"type": "array"
				},
				"quests": {
					"additionalProperties": false,
					"properties": {
						"alice_1": {
							"items": {
								"type": "boolean"
							},
							"type": "array"
						},
						"grandma_1": {
							"items": {
								"type": "boolean"
							},
							"type": "array"
						},
						"jack_1": {
							"items": {
								"type": "boolean"
							},
							"type": "array"
						}
					},
					"type": "object"
				},
				"selectedSeed": {
					"anyOf": [
						{
							"type": "null"
						},
						{
							"$ref": "#/definitions/crops"
						}
					]
				},
				"settings": {
					"additionalProperties": false,
					"properties": {
						"ambianceMute": {
							"type": "boolean"
						},
						"ambianceVolume": {
							"type": "number"
						},
						"controls": {
							"enum": [
								"mouse",
								"keyboard"
							],
							"type": "string"
						},
						"difficulty": {
							"enum": [
								"normal",
								"easy"
							],
							"type": "string"
						},
						"disableShadows": {
							"type": "boolean"
						},
						"fullscreen": {
							"type": [
								"boolean",
								"null"
							]
						},
						"lockCamera": {
							"type": "boolean"
						},
						"musicMute": {
							"type": "boolean"
						},
						"musicVolume": {
							"type": "number"
						},
						"mute": {
							"type": "boolean"
						},
						"showControls": {
							"type": "boolean"
						},
						"soundEffectsMute": {
							"type": "boolean"
						},
						"soundEffectsVolume": {
							"type": "number"
						},
						"uiScale": {
							"type": "number"
						},
						"volume": {
							"type": "number"
						}
					},
					"required": [
						"volume",
						"mute",
						"fullscreen",
						"controls",
						"showControls",
						"musicVolume",
						"musicMute",
						"ambianceVolume",
						"ambianceMute",
						"soundEffectsVolume",
						"soundEffectsMute",
						"disableShadows",
						"lockCamera",
						"uiScale",
						"difficulty"
					],
					"type": "object"
				},
				"unlockedPaths": {
					"type": "number"
				},
				"unlockedRecipes": {
					"items": {
						"$ref": "#/definitions/items"
					},
					"type": "array"
				}
			},
			"required": [
				"crops",
				"playerPosition",
				"playerRotation",
				"quests",
				"selectedSeed",
				"inventories",
				"modifiers",
				"settings",
				"unlockedRecipes",
				"unlockedPaths",
				"acorns",
				"daytime"
			],
			"type": "object"
		},
		"crops": {
			"enum": [
				"carrot",
				"beet",
				"tomato",
				"lettuce",
				"pumpkin",
				"wheat",
				"haricot",
				"magic_bean"
			],
			"type": "string"
		},
		"items": {
			"enum": [
				"acorn",
				"beet",
				"beet_seeds",
				"beetroot_salad",
				"butter",
				"carrot",
				"carrot_cake",
				"carrot_seeds",
				"carrot_soup",
				"cinnamon",
				"cookie",
				"egg",
				"flan",
				"flour",
				"ham",
				"ham_honey",
				"haricot",
				"honey",
				"honey_glazed_carrot",
				"hummus",
				"lettuce",
				"lettuce_seeds",
				"magic_bean",
				"milk",
				"parsley",
				"pumpkin",
				"pumpkin_bowl",
				"pumpkin_bread",
				"pumpkin_seeds",
				"recipe",
				"roasted_carrot",
				"slime_bread",
				"slime_dough",
				"slime_dumpling",
				"steak",
				"strawberry",
				"strawberry_pie",
				"sugar",
				"tomato",
				"tomato_seeds",
				"tomato_soup",
				"trout",
				"wheat",
				"wheat_seeds"
			],
			"type": "string"
		}
	}
}