{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "Fabled Recipes Level editor",
  "version": "0.1.0",
  "identifier": "Fabled-recipes-Level-editor",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:3000",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "Fabled Recipes Level editor",
        "width": 800,
        "height": 600,
        "resizable": true,
        "maximized": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }  
}
